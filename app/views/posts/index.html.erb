<div>
  <% @posts.each do |post|%>
    <%= render partial: "shared/post", locals: { post: post } %>
  <%end%>
</div>
<div>
  <ul>Find people to follow:</ul>
  <% User.all.each do |user|%>
    <%if user.profile && user != current_user%>
      <li><%= link_to "#{user.profile.first_name} #{user.profile.last_name}", profile_url(user.profile) %></li>
    <%end%>
  <%end%>
</div>
